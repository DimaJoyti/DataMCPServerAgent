   "source": [
    "## Data Pipeline Explorer\n",
    "\n",
    "Explore the different components of the data pipeline system:"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Data pipeline components\n",
    "pipeline_components = {\n",
    "    \"Data Ingestion\": {\n",
    "        \"description\": \"Batch and streaming data ingestion from multiple sources\",\n",
    "        \"sources\": [\n",
    "            \"PostgreSQL, MySQL, SQLite, MongoDB\",\n",
    "            \"CSV, JSON, Parquet, Excel files\",\n",
    "            \"REST APIs with authentication\",\n",
    "            \"Apache Kafka, Redis Streams\"\n",
    "        ],\n",
    "        \"example_command\": \"python examples/data_pipeline_example.py --source database\",\n",
    "        \"config_file\": \"configs/data_sources.yaml\"\n",
    "    },\n",
    "    \"Data Transformation\": {\n",
    "        \"description\": \"ETL/ELT pipelines with validation and quality checks\",\n",
    "        \"capabilities\": [\n",
    "            \"Schema validation and enforcement\",\n",
    "            \"Data cleaning and normalization\",\n",
    "            \"Custom transformation functions\",\n",
    "            \"Quality checks and monitoring\"\n",
    "        ],\n",
    "        \"example_command\": \"python -m src.data_pipeline.transform --config transform.yaml\",\n",
    "        \"config_file\": \"configs/transformations.yaml\"\n",
    "    },\n",
    "    \"Processing Engines\": {\n",
    "        \"description\": \"Parallel batch processing and real-time stream processing\",\n",
    "        \"features\": [\n",
    "            \"Distributed batch processing\",\n",
    "            \"Real-time stream processing\",\n",
    "            \"Auto-scaling based on load\",\n",
    "            \"Resource optimization\"\n",
    "        ],\n",
    "        \"example_command\": \"python -m src.data_pipeline.batch_processor --workers 4\",\n",
    "        \"config_file\": \"configs/processing.yaml\"\n",
    "    },\n",
    "    \"Pipeline Orchestration\": {\n",
    "        \"description\": \"Advanced workflow management with dependency resolution\",\n",
    "        \"orchestration\": [\n",
    "            \"Workflow dependency management\",\n",
    "            \"Parallel execution optimization\",\n",
    "            \"Error handling and recovery\",\n",
    "            \"Progress tracking and monitoring\"\n",
    "        ],\n",
    "        \"example_command\": \"python -m src.data_pipeline.orchestrator --workflow main.yaml\",\n",
    "        \"config_file\": \"configs/workflows.yaml\"\n",
    "    }\n",
    "}\n",
    "\n",
    "# Create component selector\n",
    "component_selector = widgets.Dropdown(\n",
    "    options=list(pipeline_components.keys()),\n",
    "    description='Pipeline Component:',\n",
    "    style={'description_width': 'initial'},\n",
    "    layout=widgets.Layout(width='400px')\n",
    ")\n",
    "\n",
    "component_info_output = widgets.Output(\n",
    "    layout=widgets.Layout(border='1px solid #ccc', padding='15px', width='100%')\n",
    ")\n",
    "\n",
    "def display_component_info(component_name):\n",
    "    \"\"\"Display information about the selected pipeline component.\"\"\"\n",
    "    component = pipeline_components[component_name]\n",
    "    \n",
    "    with component_info_output:\n",
    "        clear_output()\n",
    "        \n",
    "        display(HTML(f\"<h3>üîß {component_name}</h3>\"))\n",
    "        display(HTML(f\"<p><strong>Description:</strong> {component['description']}</p>\"))\n",
    "        \n",
    "        # Display component-specific information\n",
    "        for key, value in component.items():\n",
    "            if key not in ['description', 'example_command', 'config_file']:\n",
    "                display(HTML(f\"<h4>{key.replace('_', ' ').title()}:</h4>\"))\n",
    "                if isinstance(value, list):\n",
    "                    items_html = \"<ul>\" + \"\".join([f\"<li>{item}</li>\" for item in value]) + \"</ul>\"\n",
    "                    display(HTML(items_html))\n",
    "                else:\n",
    "                    display(HTML(f\"<p>{value}</p>\"))\n",
    "        \n",
    "        display(HTML(f\"<h4>Example Command:</h4>\"))\n",
    "        display(HTML(f\"<code>{component['example_command']}</code>\"))\n",
    "        \n",
    "        display(HTML(f\"<h4>Configuration File:</h4>\"))\n",
    "        display(HTML(f\"<code>{component['config_file']}</code>\"))\n",
    "\n",
    "def on_component_change(change):\n",
    "    \"\"\"Handle component selection change.\"\"\"\n",
    "    display_component_info(change['new'])\n",
    "\n",
    "component_selector.observe(on_component_change, names='value')\n",
    "display_component_info(component_selector.value)\n",
    "\n",
    "display(widgets.VBox([component_selector, component_info_output]))"
   ]
  },
