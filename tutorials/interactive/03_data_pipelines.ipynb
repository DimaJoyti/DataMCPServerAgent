    "## Setup and Environment Check\n",
    "\n",
    "First, let's set up our environment and check system requirements."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "import os\n",
    "import sys\n",
    "import asyncio\n",
    "import ipywidgets as widgets\n",
    "from IPython.display import display, clear_output, HTML\n",
    "import json\n",
    "import requests\n",
    "from datetime import datetime\n",
    "import subprocess\n",
    "\n",
    "# Add the project root to the Python path\n",
    "project_root = os.path.abspath(os.path.join(os.getcwd(), '..', '..'))\n",
    "if project_root not in sys.path:\n",
    "    sys.path.append(project_root)\n",
    "\n",
    "print(f\"Project root: {project_root}\")\n",
    "\n",
    "# Check system requirements\n",
    "def check_system_requirements():\n",
    "    \"\"\"Check if required services are available.\"\"\"\n",
    "    requirements = {\n",
    "        'Python': sys.version_info >= (3, 8),\n",
    "        'Project Root': os.path.exists(project_root),\n",
    "        'Examples Directory': os.path.exists(os.path.join(project_root, 'examples')),\n",
    "        'Scripts Directory': os.path.exists(os.path.join(project_root, 'scripts'))\n",
    "    }\n",
    "    \n",
    "    print(\"System Requirements Check:\")\n",
    "    for req, status in requirements.items():\n",
    "        status_icon = \"‚úÖ\" if status else \"‚ùå\"\n",
    "        print(f\"  {status_icon} {req}\")\n",
    "    \n",
    "    return all(requirements.values())\n",
    "\n",
    "system_ready = check_system_requirements()\n",
    "print(f\"\\nSystem Ready: {'‚úÖ' if system_ready else '‚ùå'}\")"
   ]
  },
