    "# Web interface components\n",
    "web_interfaces = {\n",
    "    \"FastAPI REST API\": {\n",
    "        \"url\": \"http://localhost:8000\",\n",
    "        \"endpoints\": [\n",
    "            \"/docs - Interactive API documentation\",\n",
    "            \"/health - System health check\",\n",
    "            \"/agents - Agent management\",\n",
    "            \"/documents - Document processing\",\n",
    "            \"/pipelines - Data pipeline control\"\n",
    "        ],\n",
    "        \"start_command\": \"python scripts/run_api.py\"\n",
    "    },\n",
    "    \"Interactive Web UI\": {\n",
    "        \"url\": \"http://localhost:8000/ui\",\n",
    "        \"features\": [\n",
    "            \"Real-time chat interface\",\n",
    "            \"Agent configuration panel\",\n",
    "            \"Performance monitoring dashboard\",\n",
    "            \"Tool exploration interface\"\n",
    "        ],\n",
    "        \"start_command\": \"python scripts/start_web_interface.py\"\n",
    "    },\n",
    "    \"WebSocket API\": {\n",
    "        \"url\": \"ws://localhost:8000/ws\",\n",
    "        \"capabilities\": [\n",
    "            \"Real-time agent responses\",\n",
    "            \"Live progress updates\",\n",
    "            \"Multi-user support\",\n",
    "            \"Event streaming\"\n",
    "        ],\n",
    "        \"start_command\": \"python scripts/websocket_server.py\"\n",
    "    },\n",
    "    \"Monitoring Dashboard\": {\n",
    "        \"url\": \"http://localhost:8000/monitoring\",\n",
    "        \"metrics\": [\n",
    "            \"Response time tracking\",\n",
    "            \"Throughput monitoring\",\n",
    "            \"Error rate analysis\",\n",
    "            \"Resource utilization\"\n",
    "        ],\n",
    "        \"start_command\": \"python scripts/start_monitoring.py\"\n",
    "    }\n",
    "}\n",
    "\n",
    "# Create interface selector\n",
    "interface_selector = widgets.Dropdown(\n",
    "    options=list(web_interfaces.keys()),\n",
    "    description='Web Interface:',\n",
    "    style={'description_width': 'initial'},\n",
    "    layout=widgets.Layout(width='400px')\n",
    ")\n",
    "\n",
    "interface_info_output = widgets.Output(\n",
    "    layout=widgets.Layout(border='1px solid #ccc', padding='15px', width='100%')\n",
    ")\n",
    "\n",
    "test_connection_button = widgets.Button(\n",
    "    description='Test Connection',\n",
    "    button_style='info',\n",
    "    layout=widgets.Layout(width='150px')\n",
    ")\n",
    "\n",
    "connection_output = widgets.Output(\n",
    "    layout=widgets.Layout(border='1px solid #ddd', padding='10px', width='100%', height='100px')\n",
    ")\n",
    "\n",
    "def display_interface_info(interface_name):\n",
    "    \"\"\"Display information about the selected web interface.\"\"\"\n",
    "    interface = web_interfaces[interface_name]\n",
    "    \n",
    "    with interface_info_output:\n",
    "        clear_output()\n",
    "        \n",
    "        display(HTML(f\"<h3>üåê {interface_name}</h3>\"))\n",
    "        display(HTML(f\"<p><strong>URL:</strong> <a href='{interface['url']}' target='_blank'>{interface['url']}</a></p>\"))\n",
    "        \n",
    "        # Display interface-specific information\n",
    "        for key, value in interface.items():\n",
    "            if key not in ['url', 'start_command']:\n",
    "                display(HTML(f\"<h4>{key.replace('_', ' ').title()}:</h4>\"))\n",
    "                if isinstance(value, list):\n",
    "                    items_html = \"<ul>\" + \"\".join([f\"<li>{item}</li>\" for item in value]) + \"</ul>\"\n",
    "                    display(HTML(items_html))\n",
    "        \n",
    "        display(HTML(f\"<h4>Start Command:</h4>\"))\n",
    "        display(HTML(f\"<code>{interface['start_command']}</code>\"))\n",
    "\n",
    "def test_connection(button):\n",
    "    \"\"\"Test connection to the selected interface.\"\"\"\n",
    "    interface_name = interface_selector.value\n",
    "    interface = web_interfaces[interface_name]\n",
    "    \n",
    "    with connection_output:\n",
    "        clear_output()\n",
    "        \n",
    "        if 'ws://' in interface['url']:\n",
    "            display(HTML(\"<p>‚ö†Ô∏è WebSocket connection testing requires a WebSocket client.</p>\"))\n",
    "            display(HTML(f\"<p>Try: <code>wscat -c {interface['url']}</code></p>\"))\n",
    "            return\n",
    "        \n",
    "        try:\n",
    "            display(HTML(f\"<p>üîÑ Testing connection to {interface['url']}...</p>\"))\n",
    "            \n",
    "            # Simulate connection test (in real scenario, would make actual HTTP request)\n",
    "            # response = requests.get(interface['url'], timeout=5)\n",
    "            \n",
    "            # For demo purposes, simulate different responses\n",
    "            import random\n",
    "            if random.choice([True, False]):\n",
    "                display(HTML(\"<p>‚úÖ Connection successful!</p>\"))\n",
    "                display(HTML(f\"<p>Service is running at {interface['url']}</p>\"))\n",
    "            else:\n",
    "                display(HTML(\"<p>‚ùå Connection failed.</p>\"))\n",
    "                display(HTML(f\"<p>Make sure to start the service: <code>{interface['start_command']}</code></p>\"))\n",
    "                \n",
    "        except Exception as e:\n",
    "            display(HTML(f\"<p>‚ùå Connection error: {str(e)}</p>\"))\n",
    "            display(HTML(f\"<p>Start the service: <code>{interface['start_command']}</code></p>\"))\n",
    "\n",
    "def on_interface_change(change):\n",
    "    \"\"\"Handle interface selection change.\"\"\"\n",
    "    display_interface_info(change['new'])\n",
    "\n",
    "interface_selector.observe(on_interface_change, names='value')\n",
    "test_connection_button.on_click(test_connection)\n",
    "\n",
    "display_interface_info(interface_selector.value)\n",
    "\n",
    "display(widgets.VBox([\n",
    "    interface_selector,\n",
    "    interface_info_output,\n",
    "    test_connection_button,\n",
    "    connection_output\n",
    "]))"
   ]
